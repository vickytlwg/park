#set($layout = "nullLayout.vm")
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
  <title>支付宝支付页面</title>
  <style>
    @charset "UTF-8";
    * {
      font-family: "Microsoft YaHei",Arial,Helvetica,sans-serif,"宋体";
      margin: 0;
      padding: 0; 
    }
    body {
      position: relative;
      background-color: #f4f3f8; 
    }
    .container {
      width: 92%;
      padding: 2% 4%;
      background-color: #fff;
      font-size: 14px; 
    }
    .container .header {
      position: relative;
      padding: 1% 0 3% 0;
      text-align: center; 
    }
    .container .header a {
      color: #0f8ce9;
      position: absolute;
      text-decoration: none;
      left: 8px;
      top: 5px; 
    }
    .container .header a:after, .container .header a:before {
      border: 8px solid transparent;
      border-right: 8px solid #fff;
      width: 0;
      height: 0;
      position: absolute;
      top: 2px;
      left: -18px;
      content: ' '; 
    }
    .container .header a:before {
      border-right-color: #0f8ce9;
      left: -20px; 
    }
    .container .header p {
      display: inline-block;
      font-size: 18px; 
    }
    .container .main-info .park-name {
      margin: 1% 0 5% 0;
      text-align: center; 
    }
    .container .main-info .park-name img, .container .main-info .park-name p {
      display: inline-block;
      vertical-align: middle; 
    }
    .container .main-info .park-name img {
      width: 16px; 
    }
    .container .main-info .park-name p {
      font-size: 14px;
      height: 30px;
      line-height: 30px; 
    }
    .container .main-info .park-fee {
      font-size: 30px;
      font-weight: 600;
      text-align: center; 
    }
    .container .main-info .fee-type {
      margin: 2% 0 10% 0;
      font-size: 14px;
      text-align: center; 
    }
    .container .main-info .notice {
      color: #b3b3b3;
      text-align: center;
      margin: 2% 0 4% 0; 
    }
    .container .money-info, .container .park-info {
      position: relative;
      border-bottom: 1px solid #f3f3f3; 
    }
    .container .money-info p, .container .park-info p {
      margin: 3% 0; 
    }
    .container .money-info span, .container .park-info span {
      position: absolute;
      right: 0;
      color: #a1a1a1; 
    }
    .container .park-info {
      border-bottom: none; 
    }

    .footer-info {
      width: 92%;
      padding: 1% 4% 10% 4%;
      font-size: 14px;
      border-top: 1px solid #efeff1; 
    }
    .footer-info .state {
      margin: 8% 0;
      color: #b2b2b4;
      text-align: center; 
    }
    .footer-info .state p span {
      display: inline-block;
      position: relative;
      width: 14px;
      height: 14px;
      margin-right: 2px;
      border: 1px solid #0f8ce9;
      border-radius: 50%;
      color: #0f8ce9;
      font-size: 12px;
      line-height: 14px; 
    }
    .footer-info button {
      width: 100%;
      height: 40px;
      margin-bottom: 10%;
      font-size: 18px;
      text-align: center;
      border: none;
      outline: none;
      background-color: #0f8ce9;
      border-radius: 4px;
      cursor: pointer; 
    }
    .footer-info button a {
      display: inline-block;
      width: 100%;
      height: 40px;
      line-height: 40px;
      color: #fff;
      text-decoration: none; 
    }
    .footer-info .contact-info {
      position: absolute;
      width: 100%;
      bottom: 1%;
      left: 50%;
      -webkit-transform: translate(-50%);
      transform: translate(-50%);
      text-align: center;
      color: #b2b2b4;
      font-size: 12px; 
    }
    .footer-info .contact-info p {
      margin: 1% 0; 
    }
    .footer-info .contact-info span {
      color: #0f8ce9; 
    }
  </style>
</head>
<body id="body">
  <div class="container">
    <div class="header header-pay">
      
      <p>停车付费</p>
    </div>
    <div class="main-info">
      <div class="park-name">
        <img src="http://www.iotclouddashboard.com/park/img/logo-ninebit.png" alt="logo">
        <p>没有您的停车记录</p>
      </div>
      <div class="park-fee">
        <p>0</p>
      </div>
      <div class="fee-type">
        <p>无需交费</p>
      </div>
    </div>
    <div class="money-info">

    </div>
	
    <div class="park-info">
     
    </div>
  </div>
  <div class="footer-info">
    <div class="state">
    </div>
    <div class="contact-info">
      <p>本服务由九比特提供</p>
      <p>客服电话 <span>4000-0000-165</span></p>
    </div>
  </div>
  <script>
    window.onload = function () {
      var body = document.getElementById('body');
      var viewportHeight = document.documentElement.clientHeight;
      body.style.minHeight = viewportHeight + 'px';
    }
  </script>
</body>
</html>