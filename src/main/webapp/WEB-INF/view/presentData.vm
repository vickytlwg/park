<style>
  .well {
    min-height: 250px;
    min-width: 1100px;
  }

  .panel .panel-body .row div {
    background: #fff;
    box-shadow: 0 0 3px rgba(0, 0, 0, 0.25);
    float: left;
    height: 160px;
    margin: 30px 0 0 35px;
    position: relative;
    text-align: center;
    width: 150px;
  }

  .panel .panel-body .row div h2 {
    background: #08b;
    background-image: radial-gradient(transparent 30%, rgba(0, 0, 0, 0.2));
    border: 0 solid rgba(0, 0, 0, 0.2);
    color: #fff;
    font-size: 18px;
    font-weight: bold;
    position: relative;
    text-shadow: -1px -1px 1px rgba(0, 0, 0, 0.2);
  }

  .panel .panel-body .row div.folded:before, div.folded:after {
    bottom: 26px;
    box-shadow: 0 28px 8px rgba(0, 0, 0, 0.5);
    content: "";
    height: 28px;
    position: absolute;
    width: 48%;
    z-index: -1;
  }

  .panel .panel-body .row div.folded:before {
    left: 2%;
    transform: rotate(-3deg);
  }

  .panel .panel-body .row div.folded:after {
    right: 2%;
    transform: rotate(3deg);
  }

  .panel .panel-body .row div.folded h2 {
    border-width: 1px 1px 2px;
    box-shadow: 0 2px 6px rgba(0, 0, 0, 0.5);
    margin: 0;
    padding: 4px 20px;
    position: absolute;
    right: -14px;
    top: 12px;
  }

  .panel .panel-body .row div.folded h3 {
    /*     border-width: 1px 1px 2px;
        box-shadow: 0 2px 6px rgba(0, 0, 0, 0.5); */
    margin: 0;
    padding: 5px 5px;
    position: absolute;
    left: 25%;
    top: 50%;
  }
  .panel .panel-body .row div.folded span {
       /*     border-width: 1px 1px 2px;
           box-shadow: 0 2px 6px rgba(0, 0, 0, 0.5); */
       margin: 0;
       padding: 5px 5px;
       position: absolute;
       left: 70%;
       top: 55%;
     }

  .panel .panel-body .row div.folded h2:after {
    border-width: 7px;
    border-style: solid;
    border-color: #134 transparent transparent #134;
    bottom: -15px;
    content: "";
    position: absolute;
    right: -1px;
  }

</style>
